date 
echo HELLO
abcxyz //error
echo $SHELL

cat /etc/shells

ctrl+b  //後方に１文字分移動する
ctrl+f  //前方に１文字分移動する
ctrl+a  //行頭に１文字分移動する
ctrl+e  //行末に１文字分移動する

ctrl+h  //カーソル位置の後方に１文字削除する
ctrl+d  //カーソル位置の１文字削除する
ctrl+w  //スペース区切りで１単語分削除する

/* カットとヤンク */
ctrl+k  //カーソル位置から行末までを削除する
ctrl+u  //カーソル位置から行頭までを削除する
ctrl+y  //最後に削除した内容を挿入する

ctrl+l  //画面を消去

ping localhost
ctrl+c  //強制終了

echo HELLO
ctrl+c  //強制終了

/*補完機能*/
TAB

/*コマンド履歴*/
ctrl+p  //１つ前のコマンド履歴に移動
ctrl+n  //次のコマンド履歴に移動
ctrl+r  //履歴を遡ってインクリメンタル検索
    ctrl+r //1つ前の検索結果へ移動
    ENTER  //現在の検索結果を実行
    ESC    //現在の検索結果を表示したままコマンドラインに戻る
    ctrl+g //検索結果を破棄しプロンプトに戻る
